        <form action="/contact" method="POST" action="abc.hbs" method="POST">
            <input type="text" 
            name="name" 
            id="name" 
            class="field" 
            placeholder="Name" 
            pattern="[A-Za-z0-9]+" 
            title="Letters and number name incorrect" required>
            
            <input type="email" 
            name="email" id="email" 
            class="field" 
            placeholder="john@example.com" 
            pattern="^([a-zA-Z0-9_\-\.]=)@((\[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$" 
            title="Please enter in this format john@example.com" required>
            
            <input type="number" 
            name="number" id="number" 
            class="field" 
            placeholder="85913xxxxx" 
            pattern="[0-9]{10}"
            title="Please enter only 10 digit Number" required>
             
            <textarea name="msg" id="" cols="70" rows="5" class="field" 
            placeholder="Your Massage" required></textarea>
              <button type="submit" id="submit" onsubmit="showPopup()" class="btn btn-primary" >Send Message</button>
            
        </form>
        
          
        
